@model ROS.Web.Models.ClubApplication

@{
    ViewData["Title"] = "ApplicationListItem";
}

<td>
    @Model.User.UserName
</td>
<td>
    @Model.Date.ToString("dd/MM/yyyy hh:mm")
</td>
<td>
    <a asp-action="ApproveApplication" asp-route-id="@Model.Id" class="btn btn-success">Godkänn</a>
    <a asp-action="RejectApplication" asp-route-id="@Model.Id" class="btn btn-danger ml-2">Avböj</a>
</td>
